<!-- Bold: Memberships (page) -->
{% include 'bold_mem_helper' with 'page' %}
<!-- end Bold code -->
<div class="page-width page-content">
  {%- render 'breadcrumbs' -%}

  <div class="grid">
    <div class="grid__item medium-up--four-sixths medium-up--push-one-sixth">
      <header class="section-header">
        <h1 class="section-header__title h2 text-left">{{ page.title }}</h1>
      </header>

      <div class="rte rte--nomargin">
        {{ page.content }}
      </div>
    </div>
  </div>
</div>
